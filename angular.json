{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "geo-dashboard": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../geo-server/dashboard",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "../shared-assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "../shared-assets/packages/bootstrap/css/bootstrap.min.css",
              "../shared-assets/icon/themify-icons/themify-icons.css",
              "../shared-assets/icon/icofont/css/icofont.css",
              "../shared-assets/pages/menu-search/css/component.css",
              "../shared-assets/css/style.css",
              "../shared-assets/css/jquery.mCustomScrollbar.css",
              "src/assets/css/popup.css",
              "src/styles.css"
            ],
            "scripts": [
              "../shared-assets/packages/jquery/js/jquery.min.js",
              "../shared-assets/packages/jquery-ui/js/jquery-ui.min.js",
              "../shared-assets/packages/popper.js/js/popper.min.js",
              "../shared-assets/packages/bootstrap/js/bootstrap.min.js",
              "../shared-assets/packages/jquery-slimscroll/js/jquery.slimscroll.js",
              "../shared-assets/packages/modernizr/js/modernizr.js",
              "../shared-assets/pages/widget/amchart/amcharts.min.js",
              "../shared-assets/pages/widget/amchart/serial.min.js",
              "../shared-assets/packages/chart.js/js/Chart.js",
              "../shared-assets/packages/i18next/js/i18next.min.js",
              "../shared-assets/packages/i18next-xhr-backend/js/i18nextXHRBackend.min.js",
              "../shared-assets/packages/i18next-browser-languagedetector/js/i18nextBrowserLanguageDetector.min.js",
              "../shared-assets/packages/jquery-i18next/js/jquery-i18next.min.js",
              "../shared-assets/pages/dashboard/custom-dashboard.js",
              "../shared-assets/js/SmoothScroll.js",
              "../shared-assets/js/pcoded.min.js",
              "../shared-assets/js/demo-12.js",
              "../shared-assets/js/jquery.mCustomScrollbar.concat.min.js",
              "../shared-assets/js/script.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "geo-dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "geo-dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "geo-dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "geo-dashboard-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "geo-dashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "geo-dashboard:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "geo-dashboard"
}
